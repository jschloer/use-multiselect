{
  "name": "use-multiselect",
  "version": "1.3.4",
  "description": "React hook for managing a the selection state of a set of items. Does not require full knowledge of the set of items.",
  "author": "jschloer",
  "license": "MIT",
  "repository": "jschloer/use-multiselect",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "jsnext:main": "dist/index.es.js",
  "keywords": [
    "React",
    "hooks"
  ],
  "engines": {
    "node": ">=8",
    "npm": ">=5"
  },
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "build": "rollup -c",
    "lint": "eslint --ext .ts --ext .tsx .",
    "start": "rollup -c -w",
    "prepare": "yarn run build",
    "predeploy": "cd example && yarn install && yarn run build",
    "deploy": "gh-pages -d example/build"
  },
  "dependencies": {
    "eslint-plugin-react-hooks": "^4.0.2",
    "object-hash": "^2.0.3"
  },
  "peerDependencies": {
    "react": "^16.8.0"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/runtime": "^7.3.1",
    "@types/jest": "^23.3.13",
    "@types/object-hash": "^1.3.2",
    "@types/react": "^16.7.22",
    "@typescript-eslint/eslint-plugin": "^2.15.0",
    "@typescript-eslint/parser": "^2.15.0",
    "cross-env": "^7.0.3",
    "eslint": "^7.0.0",
    "eslint-config-prettier": "^6.7.0",
    "eslint-plugin-prettier": "^3.1.1",
    "gh-pages": "^2.0.1",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.2",
    "prettier": "^2.0.5",
    "react": "^16.8.0",
    "rollup": "^1.1.2",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-peer-deps-external": "^2.2.0",
    "rollup-plugin-typescript2": "^0.24.0",
    "rollup-plugin-url": "^2.1.0",
    "typescript": "^3.6.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,css,json,md,ts}": [
      "prettier --write",
      "git add"
    ],
    "*.{ts,tsx}": [
      "eslint --fix",
      "git add"
    ]
  }
}
